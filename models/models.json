{
    "json_resources": [
        {
            "class": "EntityRP",
            "sub_resources": [
                {
                    "class": "AnimationRP",
                    "path": "$.'minecraft:client_entity'.description.animations.*",
                    "name": "animations"
                }
            ]
        },
        {
            "class": "AnimationFileRP",
            "sub_resources": [
                {
                    "class": "AnimationControllerState",
                    "path": "states.*",
                    "name": "states"
                }
            ]
        },
        {
            "class": "EntityBP",
            "properties": [
                {
                    "path": "'minecraft:entity'.description.identifier",
                    "name": "identifier"
                }
            ],
            "sub_resources": [
                {
                    "class": "ComponentGroup",
                    "path": "$.'minecraft:entity'.component_groups.*",
                    "name": "component_groups"
                },
                {
                    "class": "Component",
                    "path": "$.'minecraft:entity'.component_groups.*",
                    "name": "components"
                },
                {
                    "class": "Event",
                    "path": "$.'minecraft:entity'.events.*",
                    "name": "events"
                }
            ]
        },
        {
            "class": "ModelFile",
            "sub_resources": [
                {
                    "class": "Model",
                    "name": "models",
                    "path": "'minecraft:geometry'.[*]"
                }
            ]
        },
        {
            "class": "AnimationControllerFile",
            "sub_resources": [
                {
                    "class": "AnimationController",
                    "name": "animation_controllers",
                    "path": "animation_controllers.*"
                }
            ]
        }
    ],
    "sub_resources": [
        {
            "class": "AnimationControllerState"
        },
        {
            "class": "Model",
            "sub_resources": [
                {
                    "class": "Bone",
                    "path": "bones.[*]",
                    "name": "bones"
                }
            ]
        },
        {
            "class": "AnimationRP"
        },
        {
            "class": "AnimationController",
            "sub_resources": [
                {
                    "class": "AnimationControllerState",
                    "path": "states.*",
                    "name": "states"
                }
            ]
        },
        {
            "class": "ComponentGroup",
            "sub_resources": [
                {
                    "class": "Component",
                    "name": "components",
                    "path": "*"
                }
            ]
        },
        {
            "class": "Component",
            "parameters": [
                {
                    "name": "component_group",
                    "class": "ComponentGroup"
                }
            ]
        },
        {
            "class": "Event",
            "sub_resources": [
                {
                    "class": "ComponentGroup",
                    "name": "groups_to_add",
                    "path": "add.component_groups.[*]",
                    "getter": "self.parent.get_component_group(match.value)"
                },
                {
                    "class": "ComponentGroup",
                    "name": "groups_to_remove",
                    "path": "remove.component_groups.[*]",
                    "getter": "self.parent.get_component_group(match.value)"
                }
            ]
        },
        {
            "class": "Bone",
            "sub_resources": [
                {
                    "class": "Cube",
                    "name": "cubes",
                    "path": "cubes[*]"
                }
            ]
        },
        {
            "class": "Cube"
        }
    ]
}