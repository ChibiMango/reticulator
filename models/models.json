{
    "$schema": "./schema.json",
    "packs": [
        {
            "class": "ResourcePack",
            "json_resources": [
                {
                    "class": "AnimationFileRP",
                    "name": "animation_files",
                    "file_path": "animations"
                },
                {
                    "class": "EntityFileRP",
                    "name": "entities",
                    "file_path": "entity"
                },
                {
                    "class": "ModelFile",
                    "name": "model_files",
                    "file_path": "models"
                },
                {
                    "class": "ItemFileRP",
                    "name": "items",
                    "file_path": "items"
                }
            ]
        },
        {
            "class": "BehaviorPack",
            "json_resources": [
                {
                    "class": "SpawnRuleFile",
                    "name": "spawn_rules",
                    "file_path": "spawn_rules",
                    "getter": {
                        "name": "get_spawn_rule",
                        "property": "identifier"
                    }
                },
                {
                    "class": "RecipeFile",
                    "name": "recipes",
                    "file_path": "recipes",
                    "getter": {
                        "name": "get_recipe",
                        "property": "identifier"
                    }
                },
                {
                    "class": "EntityFileBP",
                    "name": "entities",
                    "file_path": "entities",
                    "getter": {
                        "name": "get_entity",
                        "property": "identifier"
                    }
                },
                {
                    "class": "AnimationControllerFile",
                    "name": "animation_controller_files",
                    "file_path": "animation_controllers"
                },
                {
                    "class": "LootTableFile",
                    "name": "loot_tables",
                    "file_path": "loot_tables"
                },
                {
                    "class": "ItemFileBP",
                    "name": "items",
                    "file_path": "items"
                }
            ]
        }
    ],
    "json_resources": [
        {
            "class": "RecipeFile",
            "properties": [
                {
                    "name": "identifier",
                    "json_path": "$.('minecraft:recipe_shaped'|'minecraft:recipe_shapeless'|'minecraft:recipe_brewing_mix'|'minecraft:recipe_furnace'|'minecraft:recipe_brewing_container').description.identifier"
                },
                {
                    "name": "format_version",
                    "json_path": "$.format_version"
                }
            ]
        },
        {
            "class": "SpawnRuleFile",
            "properties": [
                {
                    "name": "identifier",
                    "json_path": "$.'minecraft:spawn_rules.description'.identifier"
                },
                {
                    "name": "format_version",
                    "json_path": "$.format_version"
                }
            ]
        },
        {
            "class": "LootTableFile",
            "sub_resources": [
                {
                    "class": "LootTablePool",
                    "name": "pools",
                    "json_path": "$.pools.[*]"
                }
            ]
        },
        {
            "class": "ItemFileRP",
            "properties": [
                {
                    "json_path": "'minecraft:item'.description.identifier",
                    "name": "identifier"
                },
                {
                    "json_path": "format_version",
                    "name": "format_version"
                }
            ],
            "sub_resources": [
                {
                    "class": "Component",
                    "name": "components",
                    "json_path": "$.'minecraft:item'.components.*"
                }
            ]
        },
        {
            "class": "ItemFileBP",
            "properties": [
                {
                    "json_path": "'minecraft:item'.description.identifier",
                    "name": "identifier"
                }
            ],
            "sub_resources": [
                {
                    "class": "Component",
                    "name": "components",
                    "json_path": "$.'minecraft:item'.components"
                }
            ]
        },
        {
            "class": "EntityFileRP",
            "sub_resources": [
                {
                    "class": "AnimationRP",
                    "json_path": "$.'minecraft:client_entity'.description.animations.*",
                    "name": "animations"
                }
            ]
        },
        {
            "class": "AnimationFileRP",
            "properties": [
                {
                    "name": "format_version",
                    "json_path": "$.format_version"
                }
            ],
            "sub_resources": [
                {
                    "class": "AnimationRP",
                    "json_path": "$.animations.*",
                    "name": "animations"
                }
            ]
        },
        {
            "class": "EntityFileBP",
            "properties": [
                {
                    "json_path": "$.format_version",
                    "name": "format_version"
                },
                {
                    "json_path": "$.'minecraft:entity'.description.identifier",
                    "name": "identifier"
                }
            ],
            "sub_resources": [
                {
                    "class": "ComponentGroup",
                    "json_path": "$.'minecraft:entity'.component_groups.*",
                    "name": "component_groups"
                },
                {
                    "class": "Component",
                    "json_path": "$.'minecraft:entity'.components.*",
                    "name": "components",
                    "getter": {
                        "name": "get_component",
                        "property": "id"
                    }
                },
                {
                    "class": "Event",
                    "json_path": "$.'minecraft:entity'.events.*",
                    "name": "events"
                }
            ]
        },
        {
            "class": "ModelFile",
            "sub_resources": [
                {
                    "class": "Model",
                    "name": "models",
                    "json_path": "'minecraft:geometry'.[*]"
                }
            ]
        },
        {
            "class": "AnimationControllerFile",
            "sub_resources": [
                {
                    "class": "AnimationController",
                    "name": "animation_controllers",
                    "json_path": "animation_controllers.*"
                }
            ]
        }
    ],
    "sub_resources": [
        {
            "class": "LootTablePool"
        },
        {
            "class": "AnimationControllerState"
        },
        {
            "class": "Model",
            "properties": [
                {
                    "name": "identifier",
                    "json_path": "$.description.identifier"
                }
            ],
            "sub_resources": [
                {
                    "class": "Bone",
                    "json_path": "bones.[*]",
                    "name": "bones"
                }
            ]
        },
        {
            "class": "AnimationRP"
        },
        {
            "class": "AnimationController",
            "sub_resources": [
                {
                    "class": "AnimationControllerState",
                    "json_path": "states.*",
                    "name": "states"
                }
            ]
        },
        {
            "class": "ComponentGroup",
            "sub_resources": [
                {
                    "class": "Component",
                    "name": "components",
                    "json_path": "*"
                }
            ]
        },
        {
            "class": "Component",
            "optional_parameters": [
                {
                    "name": "component_group",
                    "class": "ComponentGroup"
                }
            ]
        },
        {
            "class": "Event",
            "sub_resources": [
                {
                    "class": "ComponentGroup",
                    "name": "groups_to_add",
                    "json_path": "add.component_groups.[*]",
                    "getter_argument": "self.parent.get_component_group(match.value)"
                },
                {
                    "class": "ComponentGroup",
                    "name": "groups_to_remove",
                    "json_path": "remove.component_groups.[*]",
                    "getter_argument": "self.parent.get_component_group(match.value)"
                }
            ]
        },
        {
            "class": "Bone",
            "sub_resources": [
                {
                    "class": "Cube",
                    "name": "cubes",
                    "json_path": "cubes[*]"
                }
            ]
        },
        {
            "class": "Cube"
        }
    ]
}