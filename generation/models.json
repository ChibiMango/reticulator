{
    "$schema": "./model-schema.json",
    "packs": [
        {
            "class": "ResourcePack",
            "sub_resources": [
                {
                    "class": "RenderControllerRP",
                    "name": "render_controllers",
                    "from_child": "render_controller_files",
                    "getter": {
                        "name": "get_render_controller",
                        "property": "id"
                    }
                },
                {
                    "class": "AnimationControllerRP",
                    "name": "animation_controllers",
                    "from_child": "animation_controller_files",
                    "getter": {
                        "name": "get_animation_controller",
                        "property": "id"
                    }
                },
                {
                    "class": "Model",
                    "name": "models",
                    "from_child": "model_files",
                    "getter": {
                        "name": "get_model",
                        "property": "identifier"
                    }
                }
            ],
            "json_resources": [
                {
                    "class": "ParticleFileRP",
                    "name": "particles",
                    "file_path": "particles",
                    "getter": {
                        "name": "get_particle",
                        "property": "identifier"
                    }
                },
                {
                    "class": "AttachableFileRP",
                    "name": "attachables",
                    "file_path": "attachables",
                    "getter": {
                        "name": "get_attachable",
                        "property": "identifier"
                    }
                },
                {
                    "class": "AnimationControllerFileRP",
                    "name": "animation_controller_files",
                    "file_path": "animation_controllers",
                    "getter": {
                        "name": "get_animation_controller_file",
                        "property": "file_name"
                    }
                },
                {
                    "class": "AnimationFileRP",
                    "name": "animation_files",
                    "file_path": "animations",
                    "getter": {
                        "name": "get_animation_file",
                        "property": "file_name"
                    }
                },
                {
                    "class": "EntityFileRP",
                    "name": "entities",
                    "file_path": "entity",
                    "getter": {
                        "name": "get_entity",
                        "property": "identifier"
                    }
                },
                {
                    "class": "ModelFileRP",
                    "name": "model_files",
                    "file_path": "models",
                    "getter": {
                        "name": "get_model_file",
                        "property": "file_name"
                    }
                },
                {
                    "class": "RenderControllerFileRP",
                    "name": "render_controller_files",
                    "file_path": "render_controllers",
                    "getter": {
                        "name": "get_render_controller_file",
                        "property": "file_name"
                    }
                },
                {
                    "class": "ItemFileRP",
                    "name": "items",
                    "file_path": "items"
                }
            ]
        },
        {
            "class": "BehaviorPack",
            "json_resources": [
                {
                    "class": "FunctionFile",
                    "name": "functions",
                    "file_path": "functions",
                    "file_extension": "mcfunction",
                    "getter": {
                        "name": "get_function",
                        "property": "file_path"
                    }
                },
                {
                    "class": "FeaturesFileBP",
                    "name": "features_file",
                    "file_path": "features"
                },
                {
                    "class": "FeatureRulesFileBP",
                    "name": "feature_rules_files",
                    "file_path": "feature_rules",
                    "getter": {
                        "name": "get_feature_rules_file",
                        "property": "identifier"
                    }
                },
                {
                    "class": "SpawnRuleFile",
                    "name": "spawn_rules",
                    "file_path": "spawn_rules",
                    "getter": {
                        "name": "get_spawn_rule",
                        "property": "identifier"
                    }
                },
                {
                    "class": "RecipeFile",
                    "name": "recipes",
                    "file_path": "recipes",
                    "getter": {
                        "name": "get_recipe",
                        "property": "identifier"
                    }
                },
                {
                    "class": "EntityFileBP",
                    "name": "entities",
                    "file_path": "entities",
                    "getter": {
                        "name": "get_entity",
                        "property": "identifier"
                    }
                },
                {
                    "class": "AnimationControllerFileRP",
                    "name": "animation_controller_files",
                    "file_path": "animation_controllers"
                },
                {
                    "class": "LootTableFile",
                    "name": "loot_tables",
                    "file_path": "loot_tables"
                },
                {
                    "class": "ItemFileBP",
                    "name": "items",
                    "file_path": "items",
                    "getter": {
                        "name": "get_item",
                        "property": "identifier"
                    }
                },
                {
                    "class": "BlockFileBP",
                    "name": "blocks",
                    "file_path": "blocks",
                    "getter": {
                        "name": "get_block",
                        "property": "identifier"
                    }
                }
            ]
        }
    ],
    "json_resources": [
        {
            "class": "RecipeFile",
            "properties": [
                {
                    "name": "identifier",
                    "json_path": "**/identifier"
                }
            ]
        },
        {
            "class": "ParticleFileRP",
            "properties": [
                {
                    "name": "format_version",
                    "json_path": "format_version"
                },
                {
                    "name": "identifier",
                    "json_path": "particle_effect/description/identifier"
                }
            ],
            "sub_resources": [
                {
                    "json_path": "particle_effect/components/*",
                    "class": "JsonSubResource",
                    "getter": {
                        "name": "get_component",
                        "property": "id"
                    },
                    "name": "components"
                },
                {
                    "json_path": "particle_effect/events/*",
                    "class": "JsonSubResource",
                    "getter": {
                        "name": "get_event",
                        "property": "id"
                    },
                    "name": "events"
                }
            ]
        },
        {
            "class": "AttachableFileRP",
            "properties": [
                {
                    "name": "format_version",
                    "json_path": "format_version"
                },
                {
                    "name": "identifier",
                    "json_path": "minecraft:attachable/description/identifier"
                }
            ]
        },
        {
            "class": "FeatureRulesFileBP",
            "properties": [
                {
                    "name": "identifier",
                    "json_path": "minecraft:feature_rules/description/identifier"
                },
                {
                    "name": "format_version",
                    "json_path": "format_version"
                }
            ]
        },
        {
            "class": "RenderControllerFileRP",
            "properties": [
                {
                    "name": "format_version",
                    "json_path": "format_version"
                }
            ],
            "sub_resources": [
                {
                    "class": "RenderControllerRP",
                    "json_path": "render_controllers/*",
                    "name": "render_controllers",
                    "getter": {
                        "name": "get_render_controller",
                        "property": "id"
                    }
                }
            ]
        },
        {
            "class": "AnimationControllerFileRP",
            "properties": [
                {
                    "name": "format_version",
                    "json_path": "format_version"
                }
            ],
            "sub_resources": [
                {
                    "class": "AnimationControllerRP",
                    "name": "animation_controllers",
                    "json_path": "animation_controllers/*",
                    "getter": {
                        "name": "get_animation_controller",
                        "property": "id"
                    }
                }
            ]
        },
        {
            "class": "SpawnRuleFile",
            "properties": [
                {
                    "name": "identifier",
                    "json_path": "minecraft:spawn_rules/description/identifier"
                },
                {
                    "name": "format_version",
                    "json_path": "format_version"
                }
            ]
        },
        {
            "class": "LootTableFile",
            "sub_resources": [
                {
                    "class": "LootTablePool",
                    "name": "pools",
                    "json_path": "pools/*"
                }
            ]
        },
        {
            "class": "ItemFileRP",
            "properties": [
                {
                    "json_path": "minecraft:item/description/identifier",
                    "name": "identifier"
                },
                {
                    "json_path": "format_version",
                    "name": "format_version"
                }
            ],
            "sub_resources": [
                {
                    "class": "Component",
                    "name": "components",
                    "json_path": "minecraft:item/components/*"
                }
            ]
        },
        {
            "class": "ItemFileBP",
            "properties": [
                {
                    "json_path": "minecraft:item/description/identifier",
                    "name": "identifier"
                }
            ],
            "sub_resources": [
                {
                    "class": "Component",
                    "name": "components",
                    "json_path": "minecraft:item/components"
                }
            ]
        },
        {
            "class": "BlockFileBP",
            "properties": [
                {
                    "json_path": "minecraft:block/description/identifier",
                    "name": "identifier"
                }
            ],
            "sub_resources": [
                {
                    "class": "Component",
                    "name": "components",
                    "json_path": "minecraft:block/components"
                }
            ]
        },
        {
            "class": "EntityFileRP",
            "properties": [
                {
                    "json_path": "minecraft:client_entity/description/identifier",
                    "name": "identifier"
                }
            ],
            "sub_resources": [
                {
                    "class": "AnimationRP",
                    "json_path": "minecraft:client_entity/description/animations/*",
                    "name": "animations"
                }
            ]
        },
        {
            "class": "AnimationFileRP",
            "properties": [
                {
                    "name": "format_version",
                    "json_path": "format_version"
                }
            ],
            "sub_resources": [
                {
                    "class": "AnimationRP",
                    "json_path": "animations/*",
                    "name": "animations"
                }
            ]
        },
        {
            "class": "EntityFileBP",
            "properties": [
                {
                    "json_path": "format_version",
                    "name": "format_version"
                },
                {
                    "json_path": "minecraft:entity/description/identifier",
                    "name": "identifier"
                }
            ],
            "sub_resources": [
                {
                    "class": "ComponentGroup",
                    "json_path": "minecraft:entity/component_groups/*",
                    "name": "component_groups",
                    "getter": {
                        "name": "get_component_group",
                        "property": "id"
                    },
                    "creator": {
                        "name": "create_component_group"
                    }
                },
                {
                    "class": "Component",
                    "json_path": "minecraft:entity/components/*",
                    "name": "components",
                    "getter": {
                        "name": "get_component",
                        "property": "id"
                    },
                    "creator": {
                        "name": "create_component"
                    }
                },
                {
                    "class": "Event",
                    "json_path": "minecraft:entity/events/*",
                    "name": "events"
                }
            ]
        },
        {
            "class": "ModelFileRP",
            "sub_resources": [
                {
                    "class": "Model",
                    "name": "models",
                    "json_path": "minecraft:geometry/*"
                }
            ]
        }
    ],
    "sub_resources": [
        {
            "class": "RenderControllerRP"
        },
        {
            "class": "Cube"
        },
        {
            "class": "LootTablePool"
        },
        {
            "class": "AnimationControllerStateRP"
        },
        {
            "class": "AnimationControllerRP",
            "properties": [
                {
                    "name": "initial_state",
                    "json_path": "initial_state"
                }
            ],
            "sub_resources": [
                {
                    "class": "AnimationControllerStateRP",
                    "json_path": "states/*",
                    "name": "states",
                    "getter": {
                        "name": "get_state",
                        "property": "id"
                    }
                }
            ]
        },
        {
            "class": "Model",
            "properties": [
                {
                    "name": "identifier",
                    "json_path": "description/identifier"
                }
            ],
            "sub_resources": [
                {
                    "class": "Bone",
                    "json_path": "bones/*",
                    "name": "bones"
                }
            ]
        },
        {
            "class": "AnimationRP",
            "properties": [
                {
                    "name": "loop",
                    "json_path": "loop"
                }
            ],
            "sub_resources": [
                {
                    "class": "JsonSubResource",
                    "name": "bones",
                    "json_path": "bones"
                }
            ]
        },
        {
            "class": "ComponentGroup",
            "sub_resources": [
                {
                    "class": "Component",
                    "name": "components",
                    "json_path": "*",
                    "creator": {
                        "name": "create_component",
                        "path": ""
                    }
                }
            ]
        },
        {
            "class": "Component",
            "optional_parameters": [
                {
                    "name": "component_group",
                    "class": "ComponentGroup"
                }
            ]
        },
        {
            "class": "Event",
            "sub_resources": [
                {
                    "class": "ComponentGroup",
                    "name": "groups_to_add",
                    "json_path": "add/component_groups/*"
                },
                {
                    "class": "ComponentGroup",
                    "name": "groups_to_remove",
                    "json_path": "remove/component_groups/*"
                }
            ]
        },
        {
            "class": "Bone",
            "sub_resources": [
                {
                    "class": "Cube",
                    "name": "cubes",
                    "json_path": "cubes/*"
                }
            ]
        }
    ]
}